{
  "Version": "2012-10-17",
  "Statement": [{
            "Effect": "Allow",
            "Action": ["s3:ListBucket" ],
            "Resource": ["arn:aws:s3:::bucket-development", "arn:aws:s3:::bucket-prod"]
        },
    {
      "Effect": "Allow",
      "Action": ["s3:DeleteObject", "s3:GetObject", "s3:PutObject", "s3:PutObjectAcl"],
       "Resource": [
        "arn:aws:s3:::bucket-development/*", "arn:aws:s3:::bucket-prod/*"
      ]
    }
  ]
}